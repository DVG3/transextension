{
  "manifest_version": 3,
  "name": "Text transfer",
  "version": "1",

  "description": "Transfer message to discord:3",
  "icons": {
    "16":"/images/icon.ico",
    "48":"/images/icon.ico",
    "128":"/images/icon.ico"
  },
  "action":
  {
    "default_icon":"/images/icon.ico",
    "default_popup": "/popup/geo.html"
  },
  "content_scripts":[{
    "matches":["https://www.facebook.com/*"],
    "js":["main.js"]
  }],
  "web_accessible_resources": [
    {
      "resources":["btnimg.png"],
      "matches": [ "https://www.facebook.com/*" ]
    }
  ]
}